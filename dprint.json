{
  "$schema": "https://dprint.dev/schemas/v0.json",
  "incremental": true,
  "includes": [
    "**/*.{ts,cjs,mjs,js,json}"
  ],
  "excludes": [
    "**/node_modules",
    "./dist/",
    "./npm-package/",
    "./plugin/ThirdParty/",
    "./publish/",
    "**/ui/raidboss/data",
    "**/ui/oopsyraidsy/data",
    "**/*-lock.json"
  ],
  "extends": "./dprint.base.json",
  "lineWidth": 100,
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.75.0.wasm"
  ]
}
